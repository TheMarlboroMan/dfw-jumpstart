################################################################################
#Application dependencies.

include _makefile_app

################################################################################
#Target definitions.

all: \
$(DIR_O)dummy \
$(EXECUTABLE) \

$(DIR_O)dummy:
	$(DO_DIR_OBJECTS)

$(EXECUTABLE): \
$(DIR_LIBDAN2)/libDanSDL2.a\
$(DEP_APP) \
$(DEP_FRAMEWORK) \
$(DEP_CONTROLLERS) \
$(DEP_TOOLS) \
main.cpp
	$(CC) $(CFLAGS_TARGET_FINAL) $(INCLUDES) main.cpp $(DEP_FRAMEWORK) $(DEP_APP) $(DEP_CONTROLLERS) $(DEP_TOOLS) -o $(EXECUTABLE) $(LIBRARIES) $(LINKER)

clean:
	$(DO_CLEAN)
